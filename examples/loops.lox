// Simple for loop
for (var i = 1; i < 5; i = i + 1) {
    print i * i;
}

var j = 0;

// Simple while loop
while (j < 10) {
    print j;
    j = j + 1;
}

var fact = 1;

// Another version of a simple for loop
for (var k = 1; k <= 10; k = k + 1) {
    fact = fact * k;
    print "Factorial of ";
    print k;
    print "is: ";
    print fact;
}